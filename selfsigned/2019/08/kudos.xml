  </ts:name>
  <ts:contract interface="erc721" name="Kudos">
    <ts:address network="1">0x2aea4add166ebf38b63d09a75de1a7b94aa24163</ts:address>
  </ts:contract>
  <ts:origins>
    <!-- Define the contract which holds the token that the user will use -->
    <ts:ethereum contract="Kudos">
    </ts:ethereum>
  </ts:origins>
  <ts:selections>
    <ts:selection id="expired">
      <ts:name>
        <ts:plurals xml:lang="en">
          <ts:string quantity="one">Expired Ticket</ts:string>
          <ts:string quantity="other">Expired Tickets</ts:string>
        </ts:plurals>
      </ts:name>
      <ts:filter>expired</ts:filter>
    </ts:selection>
  </ts:selections>
  <ts:cards>
    <ts:action>
      <!-- this action is of the model confirm-back.
      It should be <ts:action model="confirm-back"> but Weiwu
      shied away from specifying that due to the likely change of design causing an upgrade path issue.
      window.onConfirm is called if user hit "confirm";
      window.close() causes the back button to be pressed.
      -->
      <ts:name>
        <ts:string xml:lang="en">Stats</ts:string>
      </ts:name>
      <ts:exclude selection="expired"/>
      <style type="text/css"/>
      <ts:view xml:lang="en">
        <script type="text/javascript" xml:base="/stats.en.shtml"><![CDATA[
class Token {
    constructor(tokenInstance) {
        this.props = tokenInstance
        document.getElementById("contractAddress").value = this.props.Kudos;
    }
}

web3.tokens.dataChanged = (oldTokens, updatedTokens) => {
    const currentTokenInstance = web3.tokens.data.currentInstance;
    document.getElementById('root').innerHTML = new Token(currentTokenInstance).render();
};
]]>
</script>
<script type="text/javascript" xml:base="/stats.en.shtml">
    document.addEventListener("DOMContentLoaded", function() {
        window.onload = function startup() {
            const instance = web3.tokens.data.currentInstance;
            document.getElementById('msg').innerHTML = `
                Kudos ID: ${instance.tokenId} <br/>
                Owned: ${instance._count} <br/>
                Contract Address: ${instance.contractAddress} <br/>
                Message Contained: ${instance.description}
            `;
        }
    })
</script>
<h3 xml:base="/stats.en.shtml">Kudos by Gitcoin</h3>
<div id="msg" xml:base="/stats.en.shtml"/>
<div id="contractAddress" xml:base="/stats.en.shtml"/>
<div id="status" xml:base="/stats.en.shtml"/>

      </ts:view>
    </ts:action>
  </ts:cards>
  <ts:attribute-types>
    <ts:attribute-type id="time" syntax="1.3.6.1.4.1.1466.115.121.1.24">
      <ts:name>
        <ts:string xml:lang="en">Time</ts:string>
      </ts:name>
      <ts:origins>
        <ts:token-id as="utf8" bitmask="0000FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF0000000000000000000000"/>
      </ts:origins>
