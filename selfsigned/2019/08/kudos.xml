    <ts:attribute-type id="byId" syntax="1.3.6.1.4.1.1466.115.121.1.15"> <!-- string -->
      <ts:origins>
        <ts:ethereum as="utf8" contract="Kudos" function="getKudosById">
          <ts:data>
            <ts:uint256 ref="tokenId"/>
          </ts:data>
        </ts:ethereum>
      </ts:origins>
    </ts:attribute-type>
    <ts:attribute-type id="clones" syntax="1.3.6.1.4.1.1466.115.121.1.15"> <!-- string -->
      <ts:origins>
        <ts:ethereum as="utf8" contract="Kudos" function="getNumClonesInWild">
          <ts:data>
