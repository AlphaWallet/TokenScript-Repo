        window.onConfirm = function() {
            window.close();
        }
    }

    render() {
        let important = "";
        if(this.props.label === "GFO") {
            important = "GFO is a non-transferable token, &lt;strong&gt;DO NOT&lt;/strong&gt; try to transfer it.\n" +
                "&lt;br&gt;&lt;br&gt;Please contact the admin if you want to convert your GFO to GFT.\n" +
                "&lt;br&gt;&lt;br&gt;The development of tokenising gf.network will happen in stages. At the current stage, gf.network is the admin of GFO and GFT."
        } else {
            important = "GFT is a transferable token, but &lt;strong&gt;DO NOT&lt;/strong&gt; transfer it to non-white-listed addresses as this will fail. " +
                "&lt;br&gt;&lt;br&gt;The development of tokenising gf.network will happen in stages. At the current stage, gf.network is the admin of GFO and GFT. "
        }
        return`
        &lt;div class="ui container"&gt;
          &lt;div class="ui segment"&gt;
            &lt;img src="data:image/png;base64,"&gt;
            &lt;span&gt;&lt;bold&gt;&lt;h3&gt;${important}&lt;/h3&gt;&lt;/bold&gt;&lt;/span&gt;
          &lt;/div&gt;
        &lt;/div&gt;
`;
    }
}

web3.tokens.dataChanged = (oldTokens, updatedTokens, tokenIdCard) =&gt; {
